{
  "name": "ai-health-system-backend",
  "version": "2.0.0",
  "description": "Advanced AI Health System Backend with Gemini API Integration - Features facial health analysis, virtual calling, and comprehensive health report analysis",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "setup": "npm install && echo 'Backend setup complete! Please configure your .env file.'",
    "health-check": "curl http://localhost:5000/api/health || echo 'Server not running'",
    "build": "echo 'No build step required for Node.js backend'",
    "clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "keywords": [
    "ai",
    "healthcare",
    "gemini",
    "facial-analysis",
    "virtual-calling",
    "health-detection",
    "medical-ai",
    "nodejs",
    "express",
    "mongodb",
    "computer-vision",
    "health-screening"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.7.1",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.10.0",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.5",
    "pdf-parse": "^1.1.1",
    "qrcode": "^1.5.3",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-node": "^4.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "@types/node": "^20.10.0",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Shane-Dias/ai-health-system"
  },
  "bugs": {
    "url": "https://github.com/Shane-Dias/ai-health-system/issues"
  },
  "homepage": "https://github.com/Shane-Dias/ai-health-system#readme",
  "eslintConfig": {
    "env": {
      "node": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!node_modules/**",
      "!coverage/**"
    ]
  }
}